- pre_70 = data.timeline.select { |t| sort_date(t) <= Date.parse("1969-12-31") }
- pre_94 = data.timeline.select { |t| sort_date(t) >= Date.parse("1970-01-01") and sort_date(t) <= Date.parse("1993-12-31") }
- all_94 = data.timeline.select { |t| sort_date(t) >= Date.parse("1994-01-01") and sort_date(t) <= Date.parse("1994-12-31") }
- all_95 = data.timeline.select { |t| sort_date(t) >= Date.parse("1995-01-01") and sort_date(t) <= Date.parse("1995-12-31") }

